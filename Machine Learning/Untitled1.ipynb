   "source": [
    "from pynput import keyboard\n",
    "\n",
    "def on_press(key):\n",
    "    try:\n",
    "        print('Key pressed: {0}'.format(key.char))\n",
    "    except AttributeError:\n",
    "        print('Special key pressed: {0}'.format(key))\n",
    "\n",
    "with keyboard.Listener(on_press=on_press) as listener:\n",
    "    listener.join()\n",
    "\n",
    "on_press()"
   ]
